# Development-only configuration
# Copy this file to .env and customize for your local environment
# DO NOT commit .env file (contains local settings)

# Node environment
NODE_ENV=development

# Logging level (error, warn, info, debug, trace)
LOG_LEVEL=info

# DuckDB default settings (can be overridden at runtime)
# Memory limit for DuckDB instances
DEFAULT_MEMORY_LIMIT=2GB

# Number of threads for DuckDB query execution
DEFAULT_THREADS=2

# Feature flags for development
# Enable Electron DevTools by default in development
VITE_ENABLE_DEV_TOOLS=true

# Enable experimental features (for testing)
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# Build configuration
# Set to 'true' to generate source maps in production builds
GENERATE_SOURCEMAP=false
